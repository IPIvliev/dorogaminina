<h1>Listing zvenos</h1>

<table>
  <tr>
    <th>Number</th>
    <th>User</th>
    <th>Free</th>
    <th>Occupy</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @zvenos.each do |zveno| %>
  <tr>
    <td><%= zveno.number %></td>
    <td><%= zveno.user_id %></td>
    <td><%= zveno.free %></td>
    <td><%= zveno.occupy %></td>
    <td><%= link_to 'Show', zveno %></td>
    <td><%= link_to 'Edit', edit_zveno_path(zveno) %></td>
    <td><%= link_to 'Destroy', zveno, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

